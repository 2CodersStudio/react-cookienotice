{
  "name": "react-cookienotice",
  "version": "3.7.0",
  "description": "An awesome cookie banner for your website",
  "author": "xavierbriole",
  "license": "MIT",
  "homepage": "https://react-cookienotice.vercel.app",
  "repository": "xavierbriole/react-cookienotice",
  "keywords": [
    "banner",
    "cookie banner",
    "cookie notice",
    "cookies",
    "gdpr",
    "notice",
    "react",
    "rgpd"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "flow": "run-s flow:annotation-check flow:check",
    "flow:annotation-check": "check-flow-annotation -x src/setupTests.js -x 'dist/**' -x 'example/**' -x 'coverage/**'",
    "flow:check": "flow check --color=always",
    "format": "prettier --write \"**/*.{js,json,css}\"",
    "prepare": "run-s build",
    "test": "run-s test:unit test:format test:lint test:build",
    "test:build": "run-s build",
    "test:format": "prettier --check \"**/*.{js,json,css}\"",
    "test:lint": "eslint .",
    "test:unit": "cross-env TZ=America/New_York CI=1 react-scripts test --env=jsdom --colors --coverage",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "react": "^16.13.1 || ^17.0.0",
    "react-dom": "^16.13.1 || ^17.0.0"
  },
  "devDependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "babel-eslint": "^10.0.3",
    "check-flow-annotation": "^1.0.4",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-standard": "^5.0.0",
    "flow-bin": "^0.159.0",
    "microbundle-crl": "^0.13.11",
    "mockdate": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "clsx": "^1.1.1",
    "styled-components": "^5.3.1"
  }
}
